[
  {
    "path": "/*",
    "element": "ListView"
  },
  {
    "path": "add",
    "element": "FormView"
  },
  {
    "path": "edit/:id",
    "element": "FormView"
  },
  {
    "path": "detail/:id",
    "element": "DetailView"
  },
  {
    "path": "schema/*",
    "element": "SchemaView",
    "children": [
      {
        "path": "add",
        "element": "SchemaModal"
      },
      {
        "path": "edit/:schemaId",
        "element": "SchemaModal"
      }
    ]
  }
]